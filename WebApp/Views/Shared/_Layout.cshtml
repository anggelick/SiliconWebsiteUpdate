<!DOCTYPE html>
<html lang="sv-se">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script defer src="https://kit.fontawesome.com/ad71ca79ed.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <title>@ViewData["Title"] - Silicon</title>
    <script defer src="~/js/site.js"></script>
    <script defer src="~/js/validation.js"></script>
    <script defer src="~/js/darkMode.js"></script>

</head>
<body>

    <div class="wrapper">

        @* header *@
        @await Html.PartialAsync("~/Views/Shared/Sections/_HeaderPartial.cshtml")

        <main>
            @RenderBody()
        </main>

        @* footer *@
        @await Html.PartialAsync("~/Views/Shared/Sections/_FooterPartial.cshtml")

    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <script src="https://apis.google.com/js/platform.js" async defer></script>

</body>
</html>